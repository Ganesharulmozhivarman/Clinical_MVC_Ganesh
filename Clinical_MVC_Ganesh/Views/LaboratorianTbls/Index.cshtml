@model IEnumerable<Clinical_MVC_Ganesh.Models.LaboratorianTbl>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <style>
        body {
            background-image: url(../../Assets/Img/Admin1.jpg);
            background-size: cover;
        }

        table {
            color: white;
        }

        h2 {
            color: white;
        }
    </style>
</head>
<body>
    <h2>Laboratorian Details</h2>
    <p class="btn btn-default btn-success">
        @Html.ActionLink("Create New", "Create", null, new { style = "color:white;" })
    </p>
    @*<p>
            @Html.ActionLink("Create New", "Create")
        </p>*@
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LabName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LabEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LabPass)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LabPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LabAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LabGen)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.LabName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LabEmail)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LabPass)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LabPhone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LabAddress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LabGen)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.LabId }, new { style = "color:lawngreen;font-weight: bold;" }) |
                    @Html.ActionLink("Details", "Details", new { id = item.LabId }, new { style = "color:lawngreen;font-weight: bold;" }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.LabId }, new { style = "color:lawngreen;font-weight: bold;" })
                </td>
            </tr>
        }

    </table>
</body>
</html>