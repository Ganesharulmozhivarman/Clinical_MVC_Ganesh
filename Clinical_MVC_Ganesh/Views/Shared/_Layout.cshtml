<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse" title="more options">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Master", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                    <li>@Html.ActionLink("Receptionist", "Index", "ReceptionistTbls")</li>
                    <li>@Html.ActionLink("Doctors", "Index", "DoctorTbls")</li>
                    <li>@Html.ActionLink("Laboratorian", "Index", "LaboratorianTbls")</li>
                    <li>@Html.ActionLink("Register User", "Index", "TblLogins")</li>

                    @*<li>@Html.ActionLink("Patient", "Index", "PatientTbls")</li>*@
                    @*<li>@Html.ActionLink("Prescrition", "Index", "PrescriptionTbls")</li>
                    <li>@Html.ActionLink("Lab Test", "Index", "LabTestTbls")</li>*@
                    <li>@Html.ActionLink("Log Out", "Login", "Account")</li>

                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p style="color:white">&copy; @DateTime.Now.Year - Ganesh - Madhan Project</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

